<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="@color/middle_blue_green"
                android:centerColor="@color/viridian_green"
                android:endColor="@color/blue_sapphire"
                android:gradientRadius="100%"
                android:type="radial"
                android:centerY="0.5"
                android:centerX="0.5"/>
            <stroke
                android:width="@dimen/button_stroke"
                android:color="@color/rich_black_fOGRA_29"/>
            <size
                android:height="@dimen/button_size"
                android:width="@dimen/button_size"/>
            <shape
                android:shape="rectangle"/>
            <corners
                android:topRightRadius="@dimen/text_radious"
                android:bottomLeftRadius="@dimen/text_radious"/>
            <padding
                android:top="@dimen/common_padding_min"
                android:bottom="@dimen/common_padding_min"
                android:left="@dimen/common_padding_min"
                android:right="@dimen/common_padding_min"/>
        </shape>
    </item>
</selector>